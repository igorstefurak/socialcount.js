{
  "name": "socialcount.js"
  , "version": "0.1.1"
  , "description": "Easily get social share counts for a url."
  , "main": "./socialcount.js"
  , "keywords": [
    "browser",
    "social",
    "count",
    "counters",
    "facebook",
    "google",
    "pinterest",
    "twitter",
    "linkedin"
  ]
  , "devDependencies": {
    "tape": "*"
    , "browserify": "*"
    , "testling": "*"
  }, "testling": {
    "files": "test/*.js"
    , "browsers": [
      "ie/6..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    ]
  }, "scripts": {
    "test": "browserify test/index.js | testling -x 'google-chrome'"
  }
}
